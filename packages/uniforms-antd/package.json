{
  "name": "uniforms-antd",
  "version": "1.17.0",
  "main": "index.js",
  "jsnext:main": "src/index.js",
  "description": "Ant Design UI components for uniforms.",
  "repository": "https://github.com/vazco/uniforms/tree/master/packages/uniforms-antd",
  "homepage": "https://github.com/vazco/uniforms/",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vazco/uniforms/issues"
  },
  "keyword": [
    "form",
    "forms",
    "meteor",
    "react",
    "react-component",
    "schema",
    "validation"
  ],
  "files": [
    "*.js",
    "src/"
  ],
  "scripts": {
    "build": "babel --out-dir . src",
    "clean": "rimraf *.js coverage .nyc_output",
    "cover": "cross-env NODE_ENV=cover nyc npm test",
    "lint": "eslint src test",
    "prepublish": "npm run build",
    "pretest": "npm run lint",
    "reset": "rimraf node_modules",
    "test": "babel-node node_modules/.bin/_mocha --require test/index.js test --reporter list"
  },
  "devDependencies": {
    "antd": "2.9.0",
    "babel-cli": "6.24.0",
    "babel-eslint": "7.2.1",
    "babel-plugin-istanbul": "4.1.1",
    "babel-plugin-transform-object-assign": "6.22.0",
    "babel-plugin-transform-react-inline-elements": "6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "0.3.2",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.0",
    "babel-preset-react": "6.23.0",
    "babel-preset-stage-0": "6.22.0",
    "babel-runtime": "6.23.0",
    "chai": "4.0.0-canary.1",
    "cross-env": "4.0.0",
    "enzyme": "2.8.0",
    "eslint": "3.19.0",
    "eslint-config-vazco": "2.4.1",
    "eslint-plugin-babel": "4.1.1",
    "eslint-plugin-brackets": "0.0.1",
    "eslint-plugin-react": "6.10.3",
    "eslint-plugin-vazco": "1.0.0",
    "jsdom": "9.12.0",
    "mocha": "3.2.0",
    "nyc": "10.2.0",
    "react": "15.4.2",
    "react-addons-test-utils": "15.4.2",
    "react-dom": "15.4.2",
    "rimraf": "2.6.1",
    "sinon": "2.1.0"
  },
  "peerDependencies": {
    "antd": "^2.7.0",
    "react": "^15.0.0 || ^0.14.0",
    "uniforms": "^1.17.0"
  },
  "babel": {
    "plugins": [
      "transform-object-assign",
      "transform-react-inline-elements",
      [
        "transform-react-remove-prop-types",
        {
          "mode": "wrap"
        }
      ],
      [
        "transform-runtime",
        {
          "polyfill": false,
          "regenerator": false
        }
      ]
    ],
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ],
    "env": {
      "cover": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "vazco"
    ]
  },
  "nyc": {
    "reporter": [
      "html"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "dependencies": {
    "babel-runtime": "^6.20.0"
  }
}
