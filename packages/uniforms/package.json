    "lodash.set": "^4.2.0",
    "lodash.xorwith": "^4.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.5.0",
    "enzyme": "^2.3.0",
    "eslint": "^2.11.0",
    "eslint-config-vazco": "^1.0.0",
