{
  "name": "uniforms-repository",
  "private": true,
  "scripts": {
    "build": "tsc --build --incremental tsconfig.build.json",
    "clean": "tsc --build --clean tsconfig.build.json && rimraf coverage",
    "coverage": "jest --coverage",
    "install": "lerna bootstrap --hoist && npm run build",
    "lint": "npm run lint:code && npm run lint:text && npm run lint:types",
    "lint:code": "eslint --ext js,ts,tsx packages website",
    "lint:text": "prettier --check '**/*.{json,md,mdx,yml}'",
    "lint:types": "npm run build",
    "prereset": "npm run clean",
    "pretest": "npm run lint",
    "publish": "npm run clean && npm run build && lerna publish",
    "reset": "rimraf node_modules packages/*/node_modules website/node_modules",
    "test": "jest"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/polyfill": "7.8.3",
    "@babel/preset-env": "7.8.4",
    "@babel/preset-react": "7.8.3",
    "@babel/preset-typescript": "7.8.3",
    "@docusaurus/core": "2.0.0-alpha.32",
    "@docusaurus/preset-classic": "2.0.0-alpha.32",
    "@material-ui/core": "4.7.0",
    "@types/classnames": "2.2.9",
    "@types/enzyme": "3.10.3",
    "@types/invariant": "2.2.30",
    "@types/jest": "24.0.23",
    "@types/lodash": "4.14.149",
    "@types/react": "^16.9.26",
    "@types/warning": "3.0.0",
    "@typescript-eslint/eslint-plugin": "2.19.0",
    "@typescript-eslint/parser": "2.19.0",
    "ajv": "6.10.2",
    "antd": "^4.0.4",
    "babel-eslint": "10.0.3",
    "classnames": "^2.0.0",
    "core-js": "3.4.5",
    "dayjs": "^1.8.23",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.15.1",
    "eslint": "6.7.2",
    "eslint-config-prettier": "6.7.0",
    "eslint-config-vazco": "5.1.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-import-resolver-typescript": "2.0.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "7.17.0",
    "eslint-plugin-vazco": "1.0.0",
    "graphql": "^14.0.0 || 0.8.0 - 14.0.0",
    "husky": "3.1.0",
    "invariant": "^2.0.0",
    "jest": "24.9.0",
    "lerna": "3.19.0",
    "lint-staged": "9.5.0",
    "lodash": "^4.0.0",
    "lz-string": "1.4.4",
    "prettier": "1.19.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-feather": "2.0.3",
    "react-frame-component": "4.1.1",
    "react-test-renderer": "16.12.0",
    "rimraf": "3.0.0",
    "simpl-schema": "^1.0.0 || ^0.0.4",
    "stylelint": "12.0.0",
    "stylelint-config-prettier": "7.0.0",
    "stylelint-prettier": "1.1.1",
    "tslib": "^1.10.0",
    "typescript": "3.8.3",
    "universal-fetch": "1.0.0",
    "warning": "^4.0.0"
  },
  "engines": {
    "npm": ">=5.0.0"
  }
