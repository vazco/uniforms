{
  "name": "uniforms-repository",
  "private": true,
  "scripts": {
    "build": "tsc --build --incremental tsconfig.build.json",
    "clean": "tsc --build --clean tsconfig.build.json && rimraf coverage",
    "coverage": "jest --coverage",
    "install": "lerna bootstrap --hoist && npm run build",
    "lint": "npm run lint:code && npm run lint:text && npm run lint:types",
    "lint:code": "eslint --ext js,ts,tsx docs packages website",
    "lint:text": "prettier --check '**/*.{json,md,mdx,yml}'",
    "lint:types": "npm run build",
    "prereset": "npm run clean",
    "pretest": "npm run lint",
    "publish": "npm run clean && npm run build && lerna publish",
    "reset": "rimraf node_modules packages/*/node_modules website/node_modules",
    "test": "jest"
  },
  "dependencies": {
    "@ant-design/icons": "4.2.1",
    "@docusaurus/core": "2.0.0-alpha.58",
    "@docusaurus/preset-classic": "2.0.0-alpha.58",
    "@material-ui/core": "4.11.0",
    "@material-ui/icons": "4.9.1",
    "@types/classnames": "2.2.10",
    "@types/enzyme": "3.10.5",
    "@types/invariant": "2.2.31",
    "@types/jest": "26.0.3",
    "@types/lodash": "4.14.149",
    "@types/warning": "3.0.0",
    "@typescript-eslint/eslint-plugin": "2.27.0",
    "@typescript-eslint/parser": "2.27.0",
    "ajv": "6.12.0",
    "antd": "4.4.0",
    "babel-eslint": "10.1.0",
    "classnames": "^2.0.0",
    "core-js": "3.6.4",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-config-vazco": "5.2.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-import-resolver-typescript": "2.0.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-vazco": "1.0.0",
    "graphql": "^14.0.0 || 0.8.0 - 14.0.0",
    "husky": "4.2.3",
    "invariant": "^2.0.0",
    "jest": "26.1.0",
    "lerna": "3.20.2",
    "lint-staged": "10.1.2",
    "lodash": "^4.0.0",
    "lz-string": "1.4.4",
    "prettier": "2.0.4",
    "raw-loader": "4.0.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-feather": "2.0.4",
    "react-frame-component": "4.1.1",
    "react-test-renderer": "16.13.1",
    "rimraf": "3.0.2",
    "simpl-schema": "^1.0.0 || ^0.0.4",
    "stylelint": "13.3.0",
    "stylelint-config-prettier": "8.0.1",
    "stylelint-prettier": "1.1.2",
    "ts-jest": "26.1.1",
    "tslib": "^1.10.0",
    "typescript": "3.9.5",
    "universal-fetch": "1.0.0",
    "warning": "^4.0.0"
  },
  "engines": {
    "npm": ">=5.0.0"
  }
}
