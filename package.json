{
  "name": "uniforms-repository",
  "private": true,
  "scripts": {
    "build": "tsc --build --incremental tsconfig.build.json",
    "clean": "tsc --build --clean tsconfig.build.json && rimraf coverage",
    "coverage": "jest --coverage",
    "install": "lerna bootstrap --hoist && npm run build",
    "lint": "npm run lint:code && npm run lint:text && npm run lint:types",
    "lint:code": "eslint --ext js,ts,tsx packages website",
    "lint:text": "prettier --check '**/*.{json,md,mdx,yml}'",
    "lint:types": "npm run build",
    "prereset": "npm run clean",
    "pretest": "npm run lint",
    "publish": "npm run clean && npm run build && lerna publish",
    "reset": "rimraf node_modules packages/*/node_modules website/node_modules",
    "test": "jest"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/polyfill": "7.4.4",
    "@babel/preset-env": "7.5.5",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.3.3",
    "@docusaurus/core": "2.0.0-alpha.32",
    "@docusaurus/preset-classic": "2.0.0-alpha.32",
    "@material-ui/core": "4.6.0",
    "@material-ui/icons": "4.5.1",
    "@types/classnames": "2.2.9",
    "@types/enzyme": "3.10.3",
    "@types/invariant": "2.2.30",
    "@types/jest": "24.0.17",
    "@types/lodash": "4.14.136",
    "@types/warning": "3.0.0",
    "@typescript-eslint/eslint-plugin": "2.6.1",
    "@typescript-eslint/parser": "2.6.1",
    "ajv": "6.10.0",
    "antd": "3.19.5",
    "babel-eslint": "10.0.3",
    "classnames": "^2.0.0",
    "core-js": "3.2.1",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "eslint": "6.6.0",
    "eslint-config-prettier": "6.5.0",
    "eslint-config-vazco": "5.1.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-import-resolver-typescript": "2.0.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "7.16.0",
    "eslint-plugin-vazco": "1.0.0",
    "graphql": "^14.0.0 || 0.8.0 - 14.0.0",
    "husky": "2.4.1",
    "invariant": "^2.0.0",
    "jest": "24.8.0",
    "lerna": "3.15.0",
    "lint-staged": "9.2.1",
    "lodash": "^4.0.0",
    "lz-string": "1.4.4",
    "prettier": "1.18.2",
    "prop-types": "^15.5.7",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-feather": "2.0.3",
    "react-frame-component": "4.1.0",
    "react-test-renderer": "16.8.6",
    "rimraf": "2.6.3",
    "simpl-schema": "^1.0.0 || ^0.0.4",
    "stylelint": "10.1.0",
    "stylelint-config-prettier": "5.2.0",
    "stylelint-prettier": "1.1.1",
    "tslib": "^1.10.0",
    "typescript": "3.7.2",
    "universal-fetch": "1.0.0",
    "warning": "^4.0.0"
  },
  "engines": {
    "npm": ">=5.0.0"
  }
